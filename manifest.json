{
  "name": "N_reader",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Make Nxxxxxx easy to read",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "permissions":["*://*/*", "<all_urls>"]
}
